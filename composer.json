{
    "name":"bright-cloud-studio/isotope-product-search",
    "description":"Adds a custom field to Contao's news items to allow unique copy for each read more button.",
    "keywords":["contao", "custom", "clickthrough", "button", "news"],
    "type": "contao-bundle",
    "homepage": "http://brightcloudstudio.com/",
    "license": "LGPL-3.0+",
    "authors":[
        {
            "name": "Bright Cloud Studio",
            "homepage": "https://brightcloudstudio.com",
            "email": "mark@brightcloudstudio.com",
            "role": "Developer"
        }
    ],
    "support":{
        "issues": "https://github.com/bright-cloud-studio/custom-clickthrough-button/issues",
        "source": "https://github.com/bright-cloud-studio/custom-clickthrough-button"
    },
    "require":{
        "php": "^7.3 || ^8.0",
        "contao/core-bundle": "^4.9 || ^5.3"
    },
    "require-dev": {
        "contao/news-bundle": "~5.3"
      },
    "extra":{
        "contao-manager-plugin": "Bcs\\CustomClickthroughButtonBundle\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "Bcs\\CustomClickthroughButtonBundle\\": "src"
        },
        "classmap": [
            "src/Resources/contao"
        ],
        "exclude-from-classmap": [
            "src/Resources/contao/config/",
            "src/Resources/contao/dca/",
            "src/Resources/contao/languages/",
            "src/Resources/contao/templates/"
        ]
    }
}
