{
    "name":"bright-cloud-studio/custom-clickthrough-button",
    "description":"Adds a custom field to Contao's news items to allow unique copy for each read more button.",
    "homepage": "http://brightcloudstudio.com/",
    "keywords":["contao", "custom", "clickthrough", "button", "news"],
    "type":"contao-module",
    "license":"LGPL-3.0-or-later",
    "authors":[
        {
            "name":"Bright Cloud Studio",
            "homepage":"https://brightcloudstudio.com"
        }
    ],
    "require":{
        "php": "^8.0",
        "contao/core-bundle": "^4.13"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Bcs\\": [
                "system/modules/custom_clickthrough_button/library/Bcs"
            ]
        }
    },
    "extra": {
        "contao": {
            "sources": {
                "system/modules/custom_clickthrough_button": "system/modules/custom_clickthrough_button"
            }
        }
    },
    "minimum-stability": "dev"
}
